{
    "name": "Weather Today",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_icon": {
            "128": "logo.png"
        },
        "default_popup": "popup.html"
        
    },
    "icons": {
        "128": "logo.png"
    },
    "permissions": [
        "activeTab",
        "geolocation"
    ],
    "background": {
        "service_worker": "script.js"
    }
}